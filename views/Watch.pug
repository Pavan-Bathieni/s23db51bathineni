extends layout

block content
  
  h1= title
  - let results=watchList
  div(class="container-fluid")
  div(class="row")
   each value,index in results
    div(class="col-sm-2" onclick=`buy(${index})`)  Watch ID:#{value.WatchId} , price : #{value.Watch_Price}, Style: #{value.Watch_Style}
   script.
    function buy(index)
    {
      alert(`Your purchased Item ${index}`)
    }
  

